pipeline{
    agent any
    stages{
        stage("Navegue ate a pasta"){
            steps{
                sh 'cd /var/lib/jenkins/workspace/BackendQRCodeGenerate'
            }
        }
    }
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}